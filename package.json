{
  "name": "@c4605/react-components",
  "description": "Some useful React components",
  "main": null,
  "repository": "https://github.com/bolasblack/react-components",
  "packageManager": "pnpm@8.6.11",
  "author": "c4605 <bolasblack@gmail.com>",
  "license": "MIT",
  "private": true,
  "workspaces": [
    "packages/*"
  ],
  "scripts": {
    "postinstall": "patch-package",
    "lint": "eslint \"packages/**/src/**/*.{js,jsx,ts,tsx}\"",
    "compile": "lerna run compile",
    "test": "pnpm lint && pnpm compile && NODE_ENV=test jest --no-cache 'packages/.*/src'",
    "release": "lerna version --sign-git-tag --conventional-commits",
    "storybook": "storybook dev -p 6006",
    "storybook:build": "pnpm compile && storybook build",
    "storybook:publish": "./scripts/publish_storybook.sh"
  },
  "prettier": "@c4605/toolconfs/prettierrc",
  "jest": {
    "testEnvironment": "./configs/jestEnv.ts",
    "setupFiles": [
      "./configs/jestSetup.ts"
    ],
    "transform": {
      "^.+\\.jsx?$": "babel-jest",
      "^.+\\.tsx?$": "ts-jest"
    },
    "moduleNameMapper": {
      "\\.(css|less|scss)$": "<rootDir>/configs/mockInlineStyleModule.ts"
    },
    "testMatch": [
      "**/?(*.)+(spec|test).ts?(x)"
    ],
    "moduleFileExtensions": [
      "ts",
      "tsx",
      "js",
      "jsx",
      "json",
      "node",
      "html"
    ]
  },
  "dependencies": {
    "@babel/core": "^7.8.3",
    "@babel/plugin-proposal-class-properties": "^7.8.3",
    "@babel/preset-env": "^7.8.3",
    "@babel/preset-react": "^7.8.3",
    "@c4605/toolconfs": "^5.1.0",
    "@commitlint/cli": "^17.0.0",
    "@commitlint/config-conventional": "^17.0.0",
    "@jest/environment": "^29.6.1",
    "@testing-library/react": "^14.0.0",
    "@testing-library/react-hooks": "^8.0.0",
    "@testing-library/user-event": "^14.4.3",
    "@types/enzyme": "^3.10.4",
    "@types/jest": "^29.0.0",
    "@types/node": "^20.0.0",
    "@types/react": "^18.0.0",
    "@types/react-dom": "^18.0.0",
    "babel-plugin-auto-import": "^1.0.5",
    "coveralls": "^3.0.9",
    "css-loader": "^6.0.0",
    "git-hook-pure": "^3.0.0",
    "identity-obj-proxy": "^3.0.0",
    "jest": "^29.0.0",
    "jest-cli": "^29.0.0",
    "jest-environment-jsdom": "^29.6.1",
    "lint-staged": "^13.0.0",
    "marked": "^5.0.0",
    "patch-package": "^8.0.0",
    "postinstall-postinstall": "^2.0.0",
    "prettier": "^3.0.0",
    "react": "^18.0.0",
    "react-dom": "^18.0.0",
    "react-test-renderer": "^18.0.0",
    "sass": "^1.25.0",
    "sass-loader": "^13.0.0",
    "style-loader": "^3.0.0",
    "ts-jest": "^29.0.0",
    "ts-loader": "^9.0.0",
    "tslint": "^5.20.1",
    "typescript": "^5.1.6"
  },
  "resolutions": {
    "@types/react": "^18.0.0",
    "@types/react-dom": "^18.0.0",
    "react": "^18.0.0",
    "react-dom": "^18.0.0",
    "react-test-renderer": "^18.0.0"
  },
  "devDependencies": {
    "@storybook/addon-essentials": "^7.0.26",
    "@storybook/addon-interactions": "^7.0.26",
    "@storybook/addon-links": "^7.0.26",
    "@storybook/blocks": "^7.0.26",
    "@storybook/react": "^7.0.26",
    "@storybook/react-vite": "^7.0.26",
    "@storybook/testing-library": "^0.2.0",
    "@typescript-eslint/eslint-plugin": "^6.0.0",
    "@typescript-eslint/experimental-utils": "^5.61.0",
    "@typescript-eslint/parser": "^6.0.0",
    "eslint": "^8.44.0",
    "eslint-config-prettier": "^8.8.0",
    "eslint-plugin-prettier": "^5.0.0-alpha.2",
    "eslint-plugin-react": "^7.32.2",
    "eslint-plugin-react-hooks": "^4.6.0",
    "eslint-plugin-storybook": "^0.6.12",
    "lerna": "^7.0.0",
    "prop-types": "^15.8.1",
    "storybook": "^7.0.26",
    "trim-right": "^1.0.1",
    "tsup": "^7.1.0",
    "vite": "^4.4.2"
  },
  "pnpm": {
    "patchedDependencies": {
      "lerna@7.1.1": "patches/lerna@7.1.1.patch"
    }
  }
}
